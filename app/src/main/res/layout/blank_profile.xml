<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pageProfile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BlankFragmentProfile" >

    <LinearLayout
        android:id="@+id/LayoutBotton"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_gravity="bottom"
        android:orientation="horizontal"
        tools:ignore="UselessParent">

        <Button
            android:id="@+id/btnBuild"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/build"
            android:textColor="#000000"
            tools:ignore="VisualLintButtonSize" />

        <Button
            android:id="@+id/btnSells"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:hapticFeedbackEnabled="false"
            android:text="@string/sells"
            android:textColor="#000000"
            tools:ignore="VisualLintButtonSize" />

        <Button
            android:id="@+id/btnProfile"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/white"
            android:text="@string/profile"
            android:textColor="#000000"
            tools:ignore="VisualLintButtonSize" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LayoutAccount"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/Avatar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            tools:src="@tools:sample/avatars"
            android:contentDescription="@string/Avatar" />

        <TextView
            android:id="@+id/textFIO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/textFIO" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LayoutEmail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/textEmail" />

        <EditText
            android:id="@+id/editEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/textEmail"
            android:inputType="textEmailAddress"
            tools:ignore="LabelFor,VisualLintTextFieldSize" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LayoutPassword"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textPassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/textPassword" />

        <EditText
            android:id="@+id/editPassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/textPassword"
            android:inputType="textPassword"
            tools:ignore="LabelFor,VisualLintTextFieldSize" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LayoutPhone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textPhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/textPhone" />

        <EditText
            android:id="@+id/editPhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/textPhone"
            android:inputType="phone"
            tools:ignore="LabelFor,VisualLintTextFieldSize" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LayoutAddress"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/textAddress"
            android:textColor="#D32F2F"
            tools:ignore="TextContrastCheck" />

        <EditText
            android:id="@+id/editAddress"
            android:layout_width="322dp"
            android:layout_height="161dp"
            android:layout_weight="1"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/textAddress"
            android:inputType="textPostalAddress"
            tools:ignore="LabelFor,VisualLintTextFieldSize" />
    </LinearLayout>

</FrameLayout>